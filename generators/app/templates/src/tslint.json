{
  "extends": [
    "tslint-config-standard",
    "tslint:latest",
    "tslint-react"
  ],
  "rules": {
    "quotemark": false,
    "ordered-imports": false,
    "indent": [
      true,
      "spaces"
    ],
    "interface-name": [
      true,
      "always-prefix"
    ],
    "jsx-no-lambda": false,
    "jsx-no-multiline-js": false,
    "max-line-length": false,
    "no-console": false,
    "no-object-literal-type-assertion": false,
    "no-submodule-imports": false,
    "no-unused-variable": false,
    "space-before-function-paren": false,
    "no-implicit-dependencies": false,
    "jsx-boolean-value": false,
    "ter-indent": [
      true,
      2
    ],
    "typedef-whitespace": [
      true,
      {
        "call-signature": "nospace",
        "index-signature": "nospace",
      },
      {
        "call-signature": "nospace",
        "index-signature": "nospace",
      }
    ],
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-module",
      "check-preblock",
      "check-rest-spread",
      "check-separator",
      "check-typecast",
      "check-type-operator"
    ]
  }
}